{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "description": "This repo serves at a boilerplate to help stand up a new nextjs app",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "node server.js",
    "postinstall": "next build",
    "lint": "eslint . -f table",
    "lint:fix": "eslint --fix . -f table",
    "test": "jest",
    "test:open": "open html-report/report.html",
    "test:cover": "jest --coverage",
    "test:cover:open": "open coverage/lcov-report/index.html",
    "format": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config prettier.config.js",
    "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config prettier.config.js",
    "pre-commit": "validate-branch-name && lint-staged",
    "analyze": "cross-env ANALYZE=true next build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o docs-build"
  },
  "dependencies": {
    "@microsoft/applicationinsights-react-js": "^3.2.0",
    "@microsoft/applicationinsights-web": "^2.7.0",
    "@next/bundle-analyzer": "^10.2.0",
    "cross-env": "^7.0.3",
    "http": "0.0.1-security",
    "next": "^11.1.2",
    "pascalcase": "^1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.25.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@bahmutov/print-env": "^2.0.2",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/jest": "^26.0.24",
    "@types/pascalcase": "^1.0.1",
    "@types/react": "^17.0.27",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-goodies": "^0.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-html-reporters": "^2.1.6",
    "jest-junit": "^12.3.0",
    "lint-staged": "^10.5.4",
    "msw": "^0.28.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3",
    "validate-branch-name": "^1.0.6",
    "whatwg-fetch": "^3.6.2"
  },
  "validate-branch-name": {
    "pattern": "^(?!([mM]aster)|([mM]ain)$).*$",
    "errorMsg": "Avoid commiting witihout a branch. See readme for details."
  },
  "storybook-deployer": {
    "gitUsername": "freakinward",
    "gitEmail": "aaron.l.ward@gmail.com",
    "commitMessage": "Deploy Storybook [skip ci]"
  }
}
