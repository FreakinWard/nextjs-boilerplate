{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "buildNumber": "set-in-ci-pipeline",
  "private": true,
  "description": "This repo serves at a boilerplate to help stand up a new nextjs app",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "next build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:prod": "node --require ./load-appInsights.js node_modules/next/dist/bin/next start",
    "start:insights": "node --require ./load-appinsights.js node_modules/next/dist/bin/next start",
    "start:pm2": "pm2 start ecosystem.config.js --no-daemon",
    "lint": "next lint . -f table",
    "lint:fix": "eslint --fix . -f table",
    "test": "jest",
    "test:report": "open html-report/report.html",
    "test:cover": "jest --coverage",
    "test:cover:report": "open coverage/lcov-report/index.html",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:report": "open cypress/reports/index.html",
    "test:e2e:verify": "cypress verify",
    "test:e2e:ci": "start-test start 3000 test:e2e",
    "test:e2e:env": "./node_modules/.bin/cypress run",
    "test:e2e:docker": "docker run -it -v $PWD:/e2e -w /e2e cypress/included:9.2.0",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "pre-commit": "validate-branch-name && lint-staged",
    "analyze": "cross-env ANALYZE=true next build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o docs-build",
    "ci:prod": "npm set-script prepare '' && npm set-script postinstall '' && npm ci --production",
    "ci:set-build-number": "json -I -f package.json -e \"this.buildNumber='$CIBUILDNUMBER'\""
  },
  "dependencies": {
    "@microsoft/applicationinsights-react-js": "^3.2.0",
    "@microsoft/applicationinsights-web": "^2.7.0",
    "@next/bundle-analyzer": "^12.0.7",
    "@types/react": "^17.0.38",
    "applicationinsights": "^2.2.1",
    "cross-env": "^7.0.3",
    "cypress-mochawesome-reporter": "^3.1.0",
    "cypress-multi-reporters": "^1.6.1",
    "dotenv": "^16.0.0",
    "http": "0.0.1-security",
    "msw": "^0.36.1",
    "next": "^12.0.7",
    "next-auth": "^4.14.0",
    "pascalcase": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.25.1",
    "typescript": "^4.5.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@next/eslint-plugin-next": "^12.0.7",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/pascalcase": "^1.0.1",
    "babel-jest": "^27.2.4",
    "babel-loader": "^8.2.2",
    "cypress": "^10.9.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "jest-html-reporters": "^2.1.6",
    "jest-junit": "^13.0.0",
    "junit-report-merger": "^3.0.5",
    "lint-staged": "^12.1.2",
    "mocha-junit-reporter": "^2.0.2",
    "next-test-api-route-handler": "^3.1.7",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "validate-branch-name": "^1.0.6",
    "whatwg-fetch": "^3.6.2"
  },
  "lint-staged": {
    "./**/*.[jt]s?(x)": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": true,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "storybook-deployer": {
    "gitUsername": "freakinward",
    "gitEmail": "aaron.l.ward@gmail.com",
    "commitMessage": "Deploy Storybook [skip ci]"
  },
  "validate-branch-name": {
    "pattern": "^(?!([mM]aster)|([mM]ain)$).*$",
    "errorMsg": "Avoid commiting witihout a branch. See readme for details."
  }
}
