{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "buildNumber": "set-in-ci-pipeline",
  "private": true,
  "description": "This repo serves at a boilerplate to help stand up a new nextjs app",
  "engines": {
    "node": "~16",
    "npm": "~8"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "next build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:prod": "node --require ./load-appInsights.js node_modules/next/dist/bin/next start",
    "start:insights": "node --require ./load-appinsights.js node_modules/next/dist/bin/next start",
    "start:pm2": "pm2 start ecosystem.config.js --no-daemon",
    "lint": "next lint . -f table",
    "lint:fix": "eslint --fix . -f table",
    "test": "jest",
    "test:report": "open html-report/report.html",
    "test:cover": "jest --coverage",
    "test:cover:report": "open coverage/lcov-report/index.html",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:report": "open cypress/reports/index.html",
    "test:e2e:verify": "cypress verify",
    "test:e2e:ci": "start-test start 3000 test:e2e",
    "test:e2e:env": "./node_modules/.bin/cypress run",
    "test:e2e:docker": "docker run -it -v $PWD:/e2e -w /e2e cypress/included:9.2.0",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "pre-commit": "validate-branch-name && lint-staged",
    "analyze": "cross-env ANALYZE=true next build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o docs-build"
  },
  "dependencies": {
    "@microsoft/applicationinsights-react-js": "^3.4.1",
    "@microsoft/applicationinsights-web": "^2.8.10",
    "@next/bundle-analyzer": "^13.2.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/storybook-deployer": "^2.8.16",
    "@types/react": "^18.0.28",
    "applicationinsights": "^2.5.0",
    "cross-env": "^7.0.3",
    "cypress-mochawesome-reporter": "^3.3.0",
    "cypress-multi-reporters": "^1.6.2",
    "dotenv": "^16.0.3",
    "http": "0.0.1-security",
    "msw": "^1.1.0",
    "next": "^13.2.3",
    "next-auth": "^4.20.1",
    "pascalcase": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "typescript": "^4.9.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@next/eslint-plugin-next": "^13.2.3",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/pascalcase": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "cypress": "^12.7.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint-config-next": "13.2.3",
    "eslint-config-prettier": "^8.7.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-html-reporters": "^3.1.4",
    "jest-junit": "^15.0.0",
    "junit-report-merger": "^5.0.0",
    "lint-staged": "^13.1.2",
    "mocha-junit-reporter": "^2.2.0",
    "next-router-mock": "^0.9.2",
    "next-test-api-route-handler": "^3.1.8",
    "prettier": "^2.8.4",
    "start-server-and-test": "^1.15.4",
    "validate-branch-name": "^1.3.0",
    "whatwg-fetch": "^3.6.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/e2e"
  },
  "lint-staged": {
    "./**/*.[jt]s?(x)": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "prettier": {
    "semi": true,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "storybook-deployer": {
    "gitUsername": "freakinward",
    "gitEmail": "aaron.l.ward@gmail.com",
    "commitMessage": "Deploy Storybook [skip ci]"
  },
  "validate-branch-name": {
    "pattern": "^(?!([mM]aster)|([mM]ain)$).*$",
    "errorMsg": "Avoid commiting witihout a branch. See readme for details."
  }
}
